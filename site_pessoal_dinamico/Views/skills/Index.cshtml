@model IEnumerable<site_pessoal_dinamico.Models.skills>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.descricao_skills)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.descricao_skills)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.skillsId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.skillsId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.skillsId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
