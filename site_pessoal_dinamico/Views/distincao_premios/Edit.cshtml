@model site_pessoal_dinamico.Models.distincao_premios

@{
    ViewData["Title"] = "Edit";
}

<h1>Edit</h1>

<h4>distincao_premios</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="distincao_premiosId" />
            <div class="form-group">
                <label asp-for="descricao_distincao" class="control-label"></label>
                <input asp-for="descricao_distincao" class="form-control" />
                <span asp-validation-for="descricao_distincao" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Guardar" class="btn btn-primary" />
                <a asp-action="Index">Cancelar</a>
            </div>
        </form>
    </div>
</div>


@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
